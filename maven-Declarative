pipeline {
    agent any
    
    environment {
        PATH = "/root/maven/apache/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/manjukumarpalled/myweb.git'
            }   
        }
        stage("Maven Build"){
            steps{
                sh "maven clean pacakage"
            }
        }
    }
}
